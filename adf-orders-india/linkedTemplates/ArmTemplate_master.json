{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-orders-india"
		},
		"ls_blob_order_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_order'"
		},
		"ls_lake_orders_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_lake_orders'"
		},
		"ls_sql_orders_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_orders'"
		},
		"ls_lake_orders_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stordersprojectaccount.dfs.core.windows.net/"
		},
		"ls_sql_orders_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "db-server-0001.database.windows.net"
		},
		"ls_sql_orders_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-srijan"
		},
		"ls_sql_orders_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dbsrijan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-orders-india_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_order_connectionString": {
						"value": "[parameters('ls_blob_order_connectionString')]"
					},
					"ls_lake_orders_accountKey": {
						"value": "[parameters('ls_lake_orders_accountKey')]"
					},
					"ls_sql_orders_password": {
						"value": "[parameters('ls_sql_orders_password')]"
					},
					"ls_lake_orders_properties_typeProperties_url": {
						"value": "[parameters('ls_lake_orders_properties_typeProperties_url')]"
					},
					"ls_sql_orders_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_orders_properties_typeProperties_server')]"
					},
					"ls_sql_orders_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_orders_properties_typeProperties_database')]"
					},
					"ls_sql_orders_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_orders_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-orders-india_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-orders-india_ArmTemplate_0')]"
			]
		}
	]
}